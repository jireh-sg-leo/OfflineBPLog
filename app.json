{
  "expo": {
    "name": "BP Log",
    "slug": "BP-Log",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EBEBD3"
    },
    "updates": {
      "fallbackToCacheTimeout": 2000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.spesnaiad.bplog",
      "buildNumber": "1.2.1",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "locales": {
      "zh-Hant": "./constants/lang/zhHK.json",
      "zh-Hans": "./constants/lang/zhCN.json",
      "en": "./constants/lang/en.json",
      "fr": "./constants/lang/fr.json",
      "es": "./constants/lang/es.json"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.spesnaiad.bplog",
      "versionCode": 11,
      "permissions":[

      ]
    }
  }
}
