{
  "expo": {
    "name": "BP Log",
    "slug": "BP-Log",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#EBEBD3"
    },
    "updates": {
      "fallbackToCacheTimeout": 2000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.spesnaiad.bplog",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.spesnaiad.bplog",
      "versionCode": 1,
      "permissions":[
        "CAMERA"
      ]
    }
  }
}
